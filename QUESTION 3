#include <stdio.h>

int main()
{
   int num1,num2,sum,diff,pro,quo;
   printf("enter 2 number");
   scanf("%d%d",&num1,&num2);
   sum= num1+num2;\
   scanf("Sum=%d\n",sum);
   diff= num1-num2;
   printf("Diff=%d\n",diff);
   pro= num1*num2;
   printf("Pro=%d\n",pro);  
   quo= num1/num2;
   printf("Quo=%d\n",quo); 
   

    return 0;
}
